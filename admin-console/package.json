    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit"
  },
  "keywords": ["vue", "admin", "dashboard", "typescript", "vite"],
  "author": "Daodao Team",
  "license": "MIT",
  "dependencies": {
    "vue": "^3.5.0",
    "vue-router": "^4.4.0",
    "pinia": "^2.2.0",
    "element-plus": "^2.11.7",
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.6.5",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "vue-echarts": "^6.6.1",
    "nprogress": "^0.2.0",
    "js-cookie": "^3.0.5",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash-es": "^4.17.12",
